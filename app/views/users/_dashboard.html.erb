
<div class="border border-dark">
<div class="container">
<table class="table table-hover">
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>Mobile number</th>
      <th>Gender</th>
      <th>Uploaded CV</th>
       <th>Edit</th>
      <th>Delete</th>
      </tr>
  </thead>


  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%=user.name%></td>
        <td><%= user.mobile_number %></td>
        <td><%= user.gender %></td>
        <td> <%= link_to 'CV', user.upload %></td>   
         <td><%= link_to 'edit', edit_user_path(user) %></td>
         <td><%= link_to 'delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
      </tr>

    </tbody>
</table>
</div>
</div>